{
    "name": "micschk/silverstripe-svg-images",
    "description": "Basic SVG Image support for Silverstripe (Assets/Uploads)",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "homepage": "http://github.com/micschk/silverstripe-svg-images",
    "authors": [
        {
            "name": "Restruct web & apps",
            "email": "dev@restruct.nl"
        }
    ],
    "require": {
        "silverstripe/cms": "~4.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "suggest": {
        "stevie-mayhew/silverstripe-svg": "Adds additional SVG Template methods"
    },
    "extra": {
        "branch-alias": {
            "dev-ss4-upgrade": "2.x-dev"
        },
        "expose": [
            "css"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripeSVGImage\\": "code/"
        }
    }
}
